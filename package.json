{
  "name": "ilana-orm",
  "version": "1.0.9",
  "description": "A fully-featured, Eloquent-style ORM for Node.js with TypeScript support",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./orm/Model": {
      "import": "./orm/Model.mjs",
      "require": "./orm/Model.js",
      "types": "./orm/Model.d.ts"
    },
    "./orm/QueryBuilder": {
      "import": "./orm/QueryBuilder.mjs",
      "require": "./orm/QueryBuilder.js",
      "types": "./orm/QueryBuilder.d.ts"
    },
    "./orm/Collection": {
      "import": "./orm/Collection.mjs",
      "require": "./orm/Collection.js",
      "types": "./orm/Collection.d.ts"
    },
    "./orm/CustomCasts": {
      "import": "./orm/CustomCasts.mjs",
      "require": "./orm/CustomCasts.js",
      "types": "./orm/CustomCasts.d.ts"
    },
    "./database/connection": {
      "import": "./database/connection.mjs",
      "require": "./database/connection.js",
      "types": "./database/connection.d.ts"
    }
  },
  "bin": {
    "ilana": "cli/ilana.js"
  },
  "scripts": {
    "build": "echo 'No build needed - using JavaScript files directly'",
    "test": "jest",
    "prepublishOnly": "echo 'Ready to publish'"
  },
  "keywords": [
    "orm",
    "nodejs",
    "javascript",
    "typescript-orm",
    "typescript",
    "database",
    "eloquent",
    "mysql",
    "postgresql",
    "sqlite",
    "query-builder",
    "migrations",
    "relationships"
  ],
  "author": "Raphael Abayomi <raphyabak@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raphyabak/ilana-orm.git"
  },
  "homepage": "https://raphwebb.mintlify.com",
  "bugs": {
    "url": "https://github.com/raphyabak/ilana-orm/issues"
  },
  "files": [
    "*.js",
    "*.mjs",
    "*.d.ts",
    "cli/**/*.js",
    "database/**/*.js",
    "database/**/*.mjs",
    "database/**/*.d.ts",
    "orm/**/*.js",
    "orm/**/*.mjs",
    "orm/**/*.d.ts",
    "README.md",
    "LICENSE",
    "ilana.png"
  ],
  "dependencies": {
    "knex": "^3.0.0",
    "uuid": "^9.0.0",
    "@faker-js/faker": "^8.0.0",
    "pg": "^8.0.0",
    "mysql2": "^3.0.0",
    "sqlite3": "^5.0.0",
    "dotenv": "^16.3.1",
    "moment-timezone": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "peerDependencies": {
    "@types/node": ">=16.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}